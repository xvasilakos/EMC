public enum StatsProperty implements IProperty {

   STATS__CONF_INTERVAL_Z("stats.conf_interval_z"),
   STATS__MIN_TIME("stats.min_time"),
   STATS__PRINT__MEAN("stats.print.mean"),
   STATS__PRINT__STDDEV("stats.print.stddev"),
   STATS__PRINT__AGGREGATES("stats.print.aggregates"),
   /**
    * Set to true for printing transient (per time) statistics csv file.
    */
   STATS__PRINT__TRANSIENT("stats.print.transient"),
   STATS__PRINT__TRANSIENT__FLUSH_PERIOD("stats.print.transient.flush_period"),
   /**
    * Averaging period.
    */
   STATS__AGGREGATES__AVG_PERIOD("stats.aggregates.avg_period"),
   STATS__OUTPUTDIR("stats.outputdir"),
   //////////////
   STATS__HANLDERS__ITERATIVE__MU__CMPT1("stats.handlers.iterative.mu.cmpt1"),
   //////////////
   STATS__GAINSTATS__CMPT2("stats.gainstats.cmpt2"),
   //////////////
   STATS__HANLDERS__ITERATIVE__SC__CMPT4__NO_POLICY("stats.handlers.iterative.sc.cmpt4.no_policy"),
   STATS__HANLDERS__ITERATIVE__SC__CMPT4("stats.handlers.iterative.sc.cmpt4"),
   STATS__HANLDERS__ITERATIVE__SC__CMPT5("stats.handlers.iterative.sc.cmpt5"),
   STATS__HANLDERS__ITERATIVE__SC__CMPT6("stats.handlers.iterative.sc.cmpt6"),
   //////////////
   STATS__HANLDERS__FIXED_SC__MONITOR_SCS("stats.handlers.fixed_sc.monitor_scs"),
   STATS__HANLDERS__FIXED_SC__CMPT0("stats.handlers.fixed_sc.cmpt0"),
   STATS__HANLDERS__FIXED_SC__CMPT0__NO_POLICIES("stats.handlers.fixed_sc.cmpt0.no_policy"),
   //////////////

   STATS__ROUNDING_DECIMAL("stats.rounding_decimal"),;
   private String _propName;

   private StatsProperty(String _propTitle) {
      _propName = _propTitle;
   }

   @Override
   public String toString() {
      return _propName + ": " + toolTip();
   }

   @Override
   public String propertyName() {
      return _propName;
   }

   @Override
   public String toolTip() {
      return "tooltip TBD"; // TODO to define per different enum value
   }
}
