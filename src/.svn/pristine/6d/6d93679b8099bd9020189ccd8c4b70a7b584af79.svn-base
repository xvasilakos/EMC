package sim.space.users.mobile;

import caching.base.AbstractCachingPolicy;
import java.util.List;
import sim.Simulation;
import sim.space.Point;

/**
 *
 * @author xvas
 */
public class TraceMUBuilder extends MUBuilder {

    private final double __dx;
    private final double __dy;

    public TraceMUBuilder(Simulation simulation, MobileGroup group,
            Point startPoint, 
            List<String> connectionPolicySC,
            Iterable<AbstractCachingPolicy> cachingPolicies, double dx, double dy) {
        super(simulation, group, startPoint, null, connectionPolicySC, cachingPolicies);
        __dx = dx;
        __dy = dy;
    }

    @Override
    public TraceMU build() {
        return new TraceMU(this);
    }

    /**
     * @return the __dx
     */
    public double getDx() {
        return __dx;
    }

    /**
     * @return the __dy
     */
    public double getDy() {
        return __dy;
    }

}
