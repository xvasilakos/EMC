package stats.handlers.iterative.sc.cmpt4.no_policy;

import sim.space.cell.smallcell.SmallCell;
import stats.handlers.BaseHandler;
import stats.handlers.ICompute4;

/**
 * @author Xenofon Vasilakos xvas@aueb.gr
 */
public class W extends BaseHandler implements ICompute4<SmallCell> {

    /**
     * @param cell
     * @return
     */
    @Override
    public final double compute4(SmallCell cell) {
        double computeAvgW = cell.getDmdLclForW().computeAvgW();
        return computeAvgW;
    }

    @Override
    public String title() {
        return "W/SC";
    }
}
